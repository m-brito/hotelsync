<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="666.0"
            prefWidth="1068.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="br.edu.ifsp.hotelsync.application.controller.CreateOwnerController">

    <VBox prefHeight="666.0" prefWidth="264.0" style="-fx-background-color: #181818;">
        <VBox alignment="TOP_CENTER" prefHeight="668.0" prefWidth="264.0" style="-fx-background-color: #181818;">
            <ImageView fitHeight="73.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true">
                <VBox.margin>
                    <Insets bottom="10.0" top="50.0" />
                </VBox.margin>
                <Image url="@icon.png" />
            </ImageView>
            <Label text="Welcome to HotelSync System" textFill="#e7e5e5">
                <VBox.margin>
                    <Insets bottom="20.0" />
                </VBox.margin>
            </Label>

            <Button fx:id="btnOverview" alignment="BASELINE_LEFT" graphicTextGap="22.0" mnemonicParsing="false"
                    onAction="#handleClicks" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@css/home.css" text="Overview" textFill="#e7e5e5">
                <opaqueInsets>
                    <Insets />
                </opaqueInsets>
                <VBox.margin>
                    <Insets />
                </VBox.margin>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="reservationButton" fx:id="btnOrders" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" onAction="#handleClicks" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@css/home.css" text="Reservation">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@images/reservation.png" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="productButton" fx:id="btnCustomers" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" onAction="#handleClicks" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@css/home.css" text="Product">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@images/product.png" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="guestButton" fx:id="btnMenus" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" onAction="#handleClicks" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@css/home.css" text="Guest">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@images/guest.png" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="roomButton" fx:id="btnPackages" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@css/home.css" text="Room">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@images/room.png" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="reportButton" fx:id="btnSettings" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@css/home.css" text="Report">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@images/report.png" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="exitButton" fx:id="btnSignout" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@css/home.css" text="Exit">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@css/home.css" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Label prefHeight="167.0" prefWidth="122.0" text="By GMVV TechSolution" textFill="WHITE" />
        </VBox>
    </VBox>

    <Pane fx:id="pnlOverview" layoutX="264.0" prefHeight="666.0" prefWidth="800.0" style="-fx-background-color: #FFFFFF;">
       <Label layoutX="127.0" layoutY="14.0" prefHeight="35.0" prefWidth="297.0" text="Register Reservation Owner" textAlignment="CENTER" textFill="#eda420">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
       <TextField fx:id="nameField" layoutX="45.0" layoutY="178.0" prefHeight="25.0" prefWidth="191.0" promptText="Name" />

      <TextField fx:id="pronounsValueField" layoutX="248.0" layoutY="178.0" prefHeight="25.0" prefWidth="94.0" promptText="Pronouns" />
      <DatePicker fx:id="birthDateDataPicker" layoutX="162.0" layoutY="218.0" prefHeight="25.0" prefWidth="179.0" promptText="Birth Date" />
      <TextField fx:id="phoneValueField" layoutX="44.0" layoutY="218.0" prefHeight="25.0" prefWidth="106.0" promptText="Phone" />
      <TextField fx:id="cpfValueField" layoutX="353.0" layoutY="178.0" prefHeight="25.0" prefWidth="130.0" promptText="CPF" />
      <TitledPane alignment="CENTER" animated="false" layoutX="44.0" layoutY="255.0" prefHeight="156.0"
                    prefWidth="440.0" text="Adress">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="438.0">
                <TextField fx:id="roadValueField" layoutX="14.0" layoutY="53.0" prefHeight="25.0" prefWidth="217.0"
                           promptText="Road"/>
                <TextField fx:id="cityValueField" layoutX="102.0" layoutY="14.0" prefHeight="25.0" prefWidth="191.0"
                           promptText="City"/>
                <ComboBox fx:id="stateComboBox" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="81.0"
                          promptText="State"/>
                <TextField fx:id="cepValueField" layoutX="300.0" layoutY="14.0" prefHeight="25.0" prefWidth="130.0"
                           promptText="CEP"/>
                <TextField fx:id="districtValueField" layoutX="239.0" layoutY="53.0" prefHeight="25.0"
                           prefWidth="191.0" promptText="District"/>
                <TextField fx:id="complementValueField" layoutX="15.0" layoutY="85.0" prefHeight="25.0"
                           prefWidth="415.0" promptText="Complement"/>
            </AnchorPane>
      </TitledPane>

      <Button id="registerButton" fx:id="registerButton" layoutX="353.0" layoutY="218.0"
                mnemonicParsing="false" onAction="#onCreateOwnerClick" text="Register" />
      <Button id="cancelButton" fx:id="cancelButton" layoutX="424.0" layoutY="218.0"
                mnemonicParsing="false" onAction="#onCancelOwnerClick" prefHeight="25.0" prefWidth="59.0" text="Cancel" />
    </Pane>
</AnchorPane>
