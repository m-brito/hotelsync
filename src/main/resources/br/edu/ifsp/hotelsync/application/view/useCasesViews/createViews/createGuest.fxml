<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="666.0" prefWidth="1068.0"
            style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="br.edu.ifsp.hotelsync.application.controller.addControllers.AddGuestController">

    <VBox prefHeight="666.0" prefWidth="264.0" style="-fx-background-color: #181818;">
        <VBox alignment="TOP_CENTER" prefHeight="668.0" prefWidth="264.0" style="-fx-background-color: #181818;">
            <ImageView fitHeight="73.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true">
                <VBox.margin>
                    <Insets bottom="10.0" top="50.0" />
                </VBox.margin>
                <Image url="@../../images/icon.png" />
            </ImageView>
            <Label text="Welcome to HotelSync System" textFill="#e7e5e5">
                <VBox.margin>
                    <Insets bottom="20.0" />
                </VBox.margin>
            </Label>

            <Button fx:id="btnOverview" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" onAction="#handleClicks" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@../../css/home.css" text="Overview" textFill="#e7e5e5">
                <opaqueInsets>
                    <Insets />
                </opaqueInsets>
                <VBox.margin>
                    <Insets />
                </VBox.margin>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="reservationButton" fx:id="btnOrders" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" onAction="#handleClicks" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@../../css/home.css" text="Reservation">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../../images/reservation.png" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="productButton" fx:id="btnCustomers" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" onAction="#handleClicks" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@../../css/home.css" text="Product">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../../images/product.png" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="guestButton" fx:id="btnMenus" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" onAction="#handleClicks" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@../../css/home.css" text="Guest">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../../images/guest.png" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="roomButton" fx:id="btnPackages" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@../../css/home.css" text="Room">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../../images/room.png" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="reportButton" fx:id="btnSettings" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@../../css/home.css" text="Report">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../../images/report.png" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Button id="exitButton" fx:id="btnSignout" alignment="BASELINE_LEFT" graphicTextGap="22.0"
                    mnemonicParsing="false" prefHeight="42.0" prefWidth="259.0" style="-fx-background-color: #181818;"
                    stylesheets="@../../css/home.css" text="Exit">
                <graphic>
                    <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../../css/home.css" />
                    </ImageView>
                </graphic>
                <padding>
                    <Insets left="50.0" />
                </padding>
            </Button>

            <Label prefHeight="167.0" prefWidth="122.0" text="By GMVV TechSolution" textFill="WHITE" />
        </VBox>
    </VBox>

    <Pane fx:id="pnlOverview" layoutX="264.0" prefHeight="666.0" prefWidth="800.0" style="-fx-background-color: #FFFFFF;">
        <Label layoutX="175.0" layoutY="14.0" prefHeight="35.0" prefWidth="122.0" text="Add Guest " textAlignment="CENTER" textFill="#eda420">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <TextField fx:id="nameField" layoutX="45.0" layoutY="206.0" prefHeight="25.0" prefWidth="191.0" promptText="Name" />
        <DatePicker fx:id="birthDateDataPicker" layoutX="245.0" layoutY="206.0" prefHeight="25.0" prefWidth="135.0" promptText="Birth Date" />
        <TextField fx:id="cpfValueField" layoutX="45.0" layoutY="243.0" prefHeight="25.0" prefWidth="191.0" promptText="CPF" />

        <Button id="registerButton" fx:id="registerButton" layoutX="245.0" layoutY="243.0"
                mnemonicParsing="false" onAction="#onAddGuestClick" text="Register" />
        <Button id="cancelButton" fx:id="cancelButton" layoutX="320.0" layoutY="243.0"
                mnemonicParsing="false" onAction="#onCancelAddGuestClick" prefHeight="25.0" prefWidth="59.0" text="Cancel" />
      <TextField fx:id="reservationOwnerField" editable="false" layoutX="45.0" layoutY="168.0"
                 prefHeight="25.0" prefWidth="334.0" promptText="Reservation Owner" />
    </Pane>
</AnchorPane>
